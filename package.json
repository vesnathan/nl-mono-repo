{
  "name": "root",
  "private": true,
  "workspaces": [
    "packages/cloudwatchlive/frontend",
    "packages/cloudwatchlive/dev-mocks",
    "packages/aws-example/frontend",
    "packages/aws-example/backend",
    "packages/cloudwatchlive/backend",
    "packages/shared",
    "packages/waf",
    "packages/deploy",
    "packages/ec2-vsc",
    "packages/the-story-hub/frontend",
    "packages/the-story-hub/backend",
    "packages/card-counting-trainer/frontend",
    "packages/card-counting-trainer/backend",
    "packages/lawn-order/frontend",
    "packages/lawn-order/backend",
    "packages/app-builder-studio/frontend",
    "packages/app-builder-studio/backend"
  ],
  "dependencies": {
    "@anthropic-ai/claude-code": "^2.0.14",
    "@aws-sdk/client-cognito-identity-provider": "3.669.0",
    "@aws-sdk/client-sts": "3.669.0",
    "dotenv": "^16.3.1",
    "global": "^4.4.0",
    "graphql": "^16.9.0",
    "knip": "^5.37.2",
    "serverless": "3"
  },
  "devDependencies": {
    "@aws-amplify/cli": "12.10.1",
    "@babel/core": "^7.24.1",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@graphql-codegen/cli": "^6.0.1",
    "@graphql-codegen/typescript": "^5.0.2",
    "@graphql-codegen/typescript-operations": "^5.0.2",
    "@types/archiver": "^6.0.3",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/validator": "^13.11.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/utils": "^6.13.1",
    "autoprefixer": "^10.4.17",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "husky": "8",
    "jest": "29.7.0",
    "lerna": "^8.2.2",
    "prettier": "^3.1.0",
    "sharp": "0.30.7",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "yargs": "17.7.2"
  },
  "scripts": {
    "prepare": "husky install",
    "build-gql": "lerna run build-gql --stream --concurrency 1",
    "reset-modules": "rm -rf node_modules/ packages/**/*/node_modules",
    "install-aws-cli-local": "./scripts/install-aws-cli-local.sh",
    "deploy": "yarn workspace @deploy/core deploy",
    "deploy:tsh:dev:replace": "DOTENV_CONFIG_PATH=.env npx ts-node packages/deploy/deploy-tsh-quick.ts",
    "deploy:tsh:dev:update": "DOTENV_CONFIG_PATH=.env npx ts-node packages/deploy/deploy-tsh-quick.ts --strategy update --skip-user-creation",
    "deploy:cct:dev:replace": "DOTENV_CONFIG_PATH=.env npx ts-node packages/deploy/deploy-cct-quick.ts",
    "deploy:cct:dev:update": "DOTENV_CONFIG_PATH=.env npx ts-node packages/deploy/deploy-cct-quick.ts --strategy update --skip-user-creation",
    "deploy:lawn-order:dev": "DOTENV_CONFIG_PATH=.env npx ts-node packages/deploy/deploy-lawn-order-quick.ts",
    "deploy:lawn-order:dev:update": "DOTENV_CONFIG_PATH=.env npx ts-node packages/deploy/deploy-lawn-order-quick.ts --strategy update",
    "deploy:abs:dev": "DOTENV_CONFIG_PATH=.env npx ts-node packages/deploy/deploy-app-builder-studio-quick.ts",
    "deploy:abs:dev:update": "DOTENV_CONFIG_PATH=.env npx ts-node packages/deploy/deploy-app-builder-studio-quick.ts --strategy update",
    "aws-package-manager": "node packages/aws-bootstrap/scripts/aws-package-manager.js",
    "build": "lerna run build --stream",
    "build:tsh": "yarn workspace tshfrontend build",
    "build:cct": "yarn workspace cctfrontend build",
    "lint": "lerna run lint --stream",
    "tsc": "lerna run tsc --stream",
    "test": "lerna run test --stream",
    "dev:cwl": "yarn workspace cwlfrontend dev:local",
    "dev:awse": "yarn workspace awsefrontend dev:local",
    "dev:tsh": "yarn workspace tshfrontend dev:local",
    "dev:cct": "yarn workspace cctfrontend dev:local",
    "dev:lawn-order": "yarn workspace lawn-order-frontend dev:local",
    "dev:abs": "yarn workspace app-builder-studio-frontend dev:local"
  }
}
