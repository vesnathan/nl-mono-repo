{
  "extends": "../../../tsconfig.next.json",
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": ".",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/constants/*": [
        "./src/constants/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/hooks/*": [
        "./src/hooks/*"
      ],
      "@/layouts/*": [
        "./src/layouts/*"
      ],
      "@/pages/*": [
        "./src/pages/*"
      ],
      "@/styles/*": [
        "./src/styles/*"
      ],
      "@/utils/*": [
        "./src/utils/*"
      ],
      "@/stores/*": [
        "./src/stores/*"
      ],
      "@/SVGR/*": [
        "./src/SVGR/*"
      ],
      "@/graphQL/*": [
        "./src/graphQL/*"
      ],
      "@shared/*": [
        "../../shared/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "../../shared/**/*.ts",
    "../../shared/scripts/buildGqlTypesOnBackend.ts",
    "../../shared/scripts/execCommandAsPromise.ts",
    "../../shared/scripts/mergeGraphqlFiles.ts",
    "next-env.d.ts",
    "src/functions/v",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
