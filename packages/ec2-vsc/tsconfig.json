{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./dist",
    "target": "ES2020",
    "lib": ["ES2020"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "types": ["node"],
    "noEmit": false
  },
  "include": [
    "src/**/*",
    "*.ts",
    "../shared/constants/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
